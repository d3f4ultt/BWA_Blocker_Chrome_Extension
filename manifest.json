{
"update_url": "https://clients2.google.com/service/update2/crx",

   "background": {
      "page": "intel.html",
      "persistent": false
   },
   "content_scripts": [ {
      "all_frames": false,
	  "exclude_globs": [ "*://*twitter.com/search?q=BWAblocker&src=typd*", "*://*twitter.com/search?q=%23BWAblocker&src=typd*" ],
      "js": [ "/jquery.min.js", "/blocker.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_idle"
   } ],
   "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
   "description": "I thought what I'd do was, I'd pretend I was one of those deaf-mutes.",
   "icons": {
      "128": "/128.png",
      "130": "/130.png",
      "16": "/16.png",
      "19": "/19.png",
      "48": "/48.png",
      "550": "/550.png"
   },
   "manifest_version": 2,
   "name": "BWA Blocker",
   "options_ui": {
      "chrome_style": true,
      "page": "hacks.html"
   },
   "page_action": {
      "default_icon": "/19.png",
      "default_title": "BWA Blocker"
   },
   "permissions": [ "tabs", "storage" ],
   "version": "1.1"
}
